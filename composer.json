{
    "name": "vanushwashere/laravel-html-css-inliner",
    "description": "Inline the CSS of your HTML",
    "license": "MIT",
    "keywords": ["laravel", "html", "css", "inliner"],
    "authors": [
        {
            "name": "Vanush Ghamaryan",
            "email": "vanush.kamaryan@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~5.0",
        "tijsverkoyen/css-to-inline-styles": "~2.0"
    },
    "require-dev" : {
        "phpunit/phpunit": "~5.7",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "VanushWasHere\\LaravelHtmlCssInliner\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "VanushWasHere\\LaravelHtmlCssInliner\\LaravelHtmlCssInlinerServiceProvider"
            ]
        }
    }
}
